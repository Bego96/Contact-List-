<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="409.0" prefWidth="571.0" xmlns="http://javafx.com/javafx/18" xmlns:fx="http://javafx.com/fxml/1" fx:controller="application.Controller">
   <children>
      <TableView fx:id="tableView" layoutX="42.0" layoutY="32.0" prefHeight="200.0" prefWidth="487.0">
        <columns>
          <TableColumn fx:id="columnName" onEditCommit="#onEditChangedName" prefWidth="151.0" text="Name" />
          <TableColumn fx:id="columnLastName" onEditCommit="#onEditChangedLastName" prefWidth="174.0" text="Last name" />
            <TableColumn fx:id="columnNumber" onEditCommit="#onEditChangedNumber" prefWidth="161.0" text="Number" />
        </columns>
      </TableView>
      <Button fx:id="Add" layoutX="42.0" layoutY="341.0" mnemonicParsing="false" onAction="#Submit" text="Add" />
      <TextField fx:id="firstName" layoutX="42.0" layoutY="284.0" />
      <TextField fx:id="lastName" layoutX="211.0" layoutY="284.0" />
      <TextField fx:id="number" layoutX="380.0" layoutY="284.0" />
      <Button fx:id="Remove" layoutX="117.0" layoutY="341.0" mnemonicParsing="false" onAction="#Delete" text="Delete" />
   </children>
</AnchorPane>
